		"compression": "^1.7.4",
		"connect": "^3.7.0",
		"cors": "^2.8.5",
		"cron": "^1.8.2",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"express-fileupload": "^1.2.1",
		"express-joi-validation": "^5.0.1",
		"express-session": "^1.17.3",
		"express-validation": "^4.1.0",
		"express-validator": "^6.14.2",
		"firebase-admin": "^10.0.0",
